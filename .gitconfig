[user]
	name = Your Name
	email = you@example.com

[core]
	editor = vim
	autocrlf = input

[color]
	ui = auto
	branch = auto
	diff = auto
	status = auto

[alias]
	st = status
	co = checkout
	br = branch
	ci = commit
	lg = log --oneline --graph --decorate --all
	last = log -1 HEAD
	unstage = reset HEAD --

[push]
	default = simple

[pull]
	rebase = false

[init]
	defaultBranch = main
